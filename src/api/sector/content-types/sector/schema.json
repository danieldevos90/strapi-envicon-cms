{
  "kind": "collectionType",
  "collectionName": "sectors",
  "info": {
    "singularName": "sector",
    "pluralName": "sectors",
    "displayName": "Sector",
    "description": "Industry sectors with detailed information and features"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "order": {
      "type": "integer",
      "default": 0
    },
    "sectorContent": {
      "type": "component",
      "repeatable": false,
      "component": "sector.content-section"
    },
    "sectorFeatures": {
      "type": "component",
      "repeatable": false,
      "component": "sector.features-section"
    },
    "sectorAccordions": {
      "type": "component",
      "repeatable": false,
      "component": "sector.accordions-section"
    },
    "cta": {
      "type": "component",
      "repeatable": false,
      "component": "page.cta-section"
    }
  }
}
