{
  "collectionName": "components_ui_menu_items",
  "info": {
    "displayName": "Menu Item",
    "description": "Navigation menu item with optional submenu support"
  },
  "options": {},
  "attributes": {
    "identifier": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "href": {
      "type": "string",
      "required": false
    },
    "hasSubmenu": {
      "type": "boolean",
      "default": false
    },
    "submenuItems": {
      "type": "component",
      "repeatable": true,
      "component": "ui.submenu-item"
    }
  }
}

